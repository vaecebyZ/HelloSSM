<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.github.vaecebyz.dao.IUserDao">

    <!--    配置查询所有-->
    <select id="findAll" resultType="io.github.vaecebyz.domain.User">

        select * from people

    </select>

    <!--    保存用户-->
    <insert id="saveUser" parameterType="io.github.vaecebyz.domain.User">

        insert into people(nickname,avatar,gender,address,info) values(#{nickname},#{avatar},#{gender},#{address},#{info});

    </insert>

</mapper>